<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">View Project</li>
  </ol>
</nav>

<div class="container col-12">
  <br>
  
  <div class="row">
    {{#each rows}}
    <div class="col-6">
      <table class="table">
        <tbody>
          <tr>
            <th scope="row">Project Name:</th>
            <td>{{this.project_name}}</td>
          </tr> 
          <tr>
            <th scope="row">Project Link:</th>
            <td>{{this.Project_Link}}</td>
          </tr>
          <tr>
            <th scope="row">Project Guide:</th>
            <td>{{this.Project_Guide}}</td>
          </tr>
          <tr>
            <th scope="row">Project Date:</th>
            <td>{{this.Project_Date}}</td>
          </tr>
        </tbody>
      </table>
    </div>
    {{/each}}
    
    <div class="col-6">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Project Members</th>
            <th scope="col">Department</th>
            <th scope="col">Email</th>
          </tr> 
        </thead>
        <tbody>
          {{#each tdrow}}
          <tr>
            <td>{{this.student_name}}</td>
            <td>{{this.Department}}</td>
            <td><a href="/deletemember/{{this.ID}}/{{this.project_id}}" class="btn btn-danger">Delete</a> <a href="/viewstudent/{{this.S_ID}}" class="btn btn-primary">Std Details</a></td>
          </tr>
          {{/each}}
        </tbody>
      </table>
      
    </div>
  </div>
</div>
