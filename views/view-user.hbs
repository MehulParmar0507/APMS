<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">View Project</li>
  </ol>
</nav>

<div class="container col-12">
  <br>
  
  <div class="row">
    {{#each rows}}
    <div class="col-6">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Project Name</th>
            <th class="fw-normal">{{this.project_name}}</th>
          </tr> 
        </thead>
      </table>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Project Link</th>
            <th class="fw-normal">{{this.Project_Link}}</th>
          </tr> 
        </thead>
      </table>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Project Guide</th>
            <th class="fw-normal">{{this.Project_Guide}}</th>
          </tr> 
        </thead>
      </table>
    </div>
   {{/each}}
    
    <div class="col-6">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Project Members</th>
            <th scope="col">Department</th>
            <th scope="col">Action</th>
          </tr> 
        </thead>
        <tbody>
          {{#each tdrow}}
          <tr>
            <td>{{this.student_name}}</td>
            <td>{{this.Department}}</td>
            <td><a href="/deletemember/{{this.ID}}/{{this.project_id}}" class="btn btn-danger">Delete</a> <a href="/viewstudent/{{this.S_ID}}" class="btn btn-primary">Std Details</a></td>
          </tr>
          {{/each}}
        </tbody>
      </table>
      <a href="/addmembers/{{this.id.id}}" class="btn btn-primary">Add Members</a>
    </div>
  </div>
  

</div>